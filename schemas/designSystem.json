{
  "$schema": "https://json-schema.org/draft-07/schema",
  "properties": {
    "tokens": {
      "additionalProperties": {
        "oneOf": [ {"type": "string"}, {"type": "number"} ]
      },
      "propertyNames": {"pattern": "^[\\w/-]+$"},
      "type": "object"
    }
  },
  "required": ["tokens"]
}
